# PressureFlow CRM - Environment Variables
# Copy this to .env and fill in your values

# Supabase credentials (from your Supabase project settings > API)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-or-service-key

# App secret for JWT tokens (change this to a random string!)
SECRET_KEY=change-this-to-a-random-secret-string-123

# Flask settings
FLASK_DEBUG=false
PORT=5000
